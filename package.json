    "es6-promise": "^4.0.5",
    "eslint": "^3.12.1",
    "eslint-config-devine": "^1.5.0",
    "eslint-loader": "^1.6.1",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.9.0",
    "image-webpack-loader": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^18.0.0",
